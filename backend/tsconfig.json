{
  "compilerOptions": {
    "types": ["node", "express"],
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "rootDir": "src",
    "resolveJsonModule": true,
    "sourceMap": true
  },
  "include": ["src"]
}
